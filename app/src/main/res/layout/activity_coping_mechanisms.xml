<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_coping_mechanisms"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.login_registernew.CopingMechanisms">

    <ImageButton
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginLeft="28dp"
        android:layout_marginStart="28dp"
        android:src="@drawable/hobby"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:backgroundTint="@android:color/transparent"
        android:id="@+id/hobby"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="34dp" />

    <ImageButton
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/coffee"
        android:id="@+id/takeTime"
        android:backgroundTint="@android:color/transparent"
        android:layout_below="@+id/toolbar"
        android:layout_alignLeft="@+id/stayHealthy"
        android:layout_alignStart="@+id/stayHealthy"
        android:layout_marginTop="22dp" />

    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/homebutton"
        android:id="@+id/homeButton"
        android:backgroundTint="@android:color/transparent"
        android:layout_alignTop="@+id/weblinks"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp" />

    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:padding="0dp"
        android:src="@drawable/weblinks"
        android:scaleType="fitCenter"
        android:id="@+id/weblinks"
        android:backgroundTint="@android:color/transparent"
        android:layout_marginRight="14dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="11dp"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/homeButton"
        android:layout_toStartOf="@+id/homeButton" />

    <android.support.v7.widget.Toolbar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="?attr/colorAccent"
        android:theme="?attr/actionBarTheme"
        android:minHeight="?attr/actionBarSize"
        android:id="@+id/toolbar"
        app:title="Coping Mechanisms"
        app:titleTextColor="@android:color/background_light"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:src="@drawable/health"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:backgroundTint="@android:color/transparent"
        android:id="@+id/stayHealthy"
        android:layout_marginBottom="25dp"
        android:layout_above="@+id/hobby"
        android:layout_alignLeft="@+id/hobby"
        android:layout_alignStart="@+id/hobby" />

    <ImageButton
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/money"
        android:backgroundTint="@android:color/transparent"
        android:id="@+id/finance"
        android:layout_marginBottom="35dp"
        android:layout_above="@+id/homeButton"
        android:layout_alignLeft="@+id/getOrganised"
        android:layout_alignStart="@+id/getOrganised" />

    <ImageButton
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/clipboard"
        android:backgroundTint="@android:color/transparent"
        android:id="@+id/getOrganised"
        android:layout_marginRight="36dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="51dp"
        android:layout_above="@+id/finance"
        android:layout_alignRight="@+id/homeButton"
        android:layout_alignEnd="@+id/homeButton" />

</RelativeLayout>

